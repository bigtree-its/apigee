<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-NotFound">
  <DisplayName>RF-NotFound</DisplayName>
  <Properties/>
  <FaultResponse>
    <Set>
      <StatusCode>404</StatusCode>
    </Set>
    <AssignVariable>
      <Name>flow.cds.error.message</Name>
      <Value>Not Found</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>flow.cds.error.code</Name>
      <Value>404.001.002</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>flow.cds.error.status</Name>
      <Value>404</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>flow.cds.error.reason</Name>
      <Value>Not Found</Value>
    </AssignVariable>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <ShortFaultReason>false</ShortFaultReason>
</RaiseFault>
