<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-InvalidArrangementId">
  <DisplayName>RF-InvalidArrangementId</DisplayName>
  <Properties/>
  <FaultResponse>
    <Set>
      <StatusCode>403</StatusCode>
    </Set>
    <AssignVariable>
      <Name>flow.myapi.error.message</Name>
      <Value>ArrangementId passed in the request URL is not in correct format</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>flow.myapi.error.code</Name>
      <Value>400.002.002</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>flow.myapi.error.status</Name>
      <Value>400</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>flow.myapi.error.reason</Name>
      <Value>Bad Request</Value>
    </AssignVariable>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>