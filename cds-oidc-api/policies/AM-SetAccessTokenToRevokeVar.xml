<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-SetAccessTokenToRevokeVar">
  <DisplayName>AM-SetAccessTokenToRevokeVar</DisplayName>
  <AssignVariable>
    <Name>theAccessTokenToRevoke</Name>
    <Template>{request.formparam.token}</Template>
  </AssignVariable>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>