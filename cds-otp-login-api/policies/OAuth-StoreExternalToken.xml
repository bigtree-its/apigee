<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 name="OAuth-StoreExternalToken">
    <DisplayName>OAuth-StoreExternalToken</DisplayName>
    <Attributes/>
    <ExternalAccessToken>cds_otp_login.forgerockAccessToken</ExternalAccessToken>
    <ExpiresIn ref="cds_otp_login.forgerockAccessTokenExpiryTimeInMs">3600000</ExpiresIn>
    <ExternalAuthorization>true</ExternalAuthorization>
    <Operation>GenerateAccessToken</Operation>
    <GenerateResponse enabled="true"/>
    <StoreToken>true</StoreToken>
    <GrantType>grantType</GrantType>
    <Scope>kvm.cdrConsentScope</Scope>
    <SupportedGrantTypes>
        <GrantType>client_credentials</GrantType>
    </SupportedGrantTypes>
    <Attributes>
        <Attribute name="consentId" ref="cds_otp_login.consentId" display="true"/>
    </Attributes>
    <Tokens/>
</OAuthV2>